:ruby
  cols_hash = {
    :name => 10
  }

.page-model-list.no-headtop.admin-upload-documents
  - if models.blank?
    .blank
      目前列表内没有内容
  - else
    - models.each do |model|
      = page_list_body :cols => cols_hash do |p|
        = p.cell :name do
          - case model
          - when UploadDocumentDir
            %a{:href=>"/admin/upload_document_dirs/#{model.id}"}= model.name
          - when UploadDocument
            %a{:href=>"/admin/upload_documents/#{model.id}"}= model.title